@section PageScripts{
    <script src="~/assets/js/validation/bootstrapValidator.js"></script>
    <script src="~/assets/js/select2/select2.js"></script>

    <script>
        function Fill_Elements() {
            document.getElementById("titleNode").value = '@Html.Raw(ViewBag.TittleNode)';
            document.getElementById("descriptionNode").value = '@Html.Raw(ViewBag.DescriptionNode)';
            document.getElementById("imageurlNode").value = '@Html.Raw(ViewBag.ImageUrlNode)';

            var Roles = '@ViewBag.RolesNode'; //declara una variable para la cadena de Roles
            var selectedValues = Roles.split(","); // Separa la cadena de Roles y los almacena en un arreglo(es necesario para indicarle al combo de roles que se ingresara mas de un parametro si fuece el caso.)
            $("#rolesNode").val(selectedValues); // se asigna la cadena

            document.getElementById("areaNode").value = '@ViewBag.AreaNode';
            document.getElementById("controllerNode").value = '@ViewBag.ControllerNode';
            document.getElementById("actionNode").value = '@ViewBag.ActionNode';
            document.getElementById("keyParentNode").value = '@ViewBag.KeyParent';
            $("#rolesNode").change();
            $("#imageurlNode").change();
            $("#keyParentNode").change();
        }
        $("#imageurlNode").select2();
        $("#keyParentNode").select2();
        $("#rolesNode").select2({
            placeholder: "Seleccione permisos",
            allowClear: true
        });


        $(document).ready(function () {
            Fill_Elements();
            $('#togglingForm').bootstrapValidator({
                message: 'El campo no es válido',
                feedbackIcons: {
                    valid: 'glyphicon glyphicon-ok',
                    invalid: 'glyphicon glyphicon-remove',
                    validating: 'glyphicon glyphicon-refresh'
                },
                submitHandler: function (validator, form, submitButton) {
                    // Do nothing
                },
                fields: {
                    titleNode: {
                        validators: {
                            notEmpty: {
                                message: 'El nombre del nodo es requerido'
                            }
                        }
                    },
                    descriptionNode: {
                        validators: {
                            notEmpty: {
                                message: 'La descripción del nodo es requerida'
                            }
                        }
                    },
                    //imageurlNode: {
                    //    validators: {
                    //        notEmpty: {
                    //            message: 'El ícono del nodo es requerido'
                    //        }
                    //    }
                    //}
                }
            })
            .find('button[data-toggle]')
            .on('click', function () {
                var $target = $($(this).attr('data-toggle'));
                // Show or hide the additional fields
                // They will or will not be validated based on their visibilities
                $target.toggle();
                if (!$target.is(':visible')) {
                    // Enable the submit buttons in case additional fields are not valid
                    $('#togglingForm').data('bootstrapValidator').disableSubmitButtons(false);
                }
            });

        });

    </script>
